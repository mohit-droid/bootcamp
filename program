NumList = []
EvenProductList = []
OddSumList = []

Integer = int(input("Enter the number of elements needed in the list: "))
for j in range(1, Integer + 1):
    value = int(input(" Enter the %d Element : " %i))
    NumList.append(value)

for m in range(Integer):
	for n in range(Integer):
    	   if((NumList[m]*NumList[n]) % 2 == 0):
	          if(NumList[m],NumList[n])||(NumList[n],NumList[m])not in EvenProductList:
	            	EvenProductList.extend([NumList[m],NumList[n]])
		
	     	if((NumList[m]+NumList[n]) % 2 != 0):
		         if(NumList[m],NumList[n])||(NumList[n],NumList[m])not in OddSumList
                OddSumList.extend([NumList[m],NumList[n]])   


print("pairs whose product is even are: ",EvenProductList)
print("pairs whose sum is odd are: ",OddSumList)
